<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:scrollbars="horizontal|vertical"
    tools:context="com.example.sacaae_mobile.ComisionConsultationDetail" >

    <TextView
        android:id="@+id/lblComisionName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="16dp"
        android:text="Comision Name"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="24dp" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lblComisionName"
        android:layout_alignRight="@+id/lblComisionName"
        android:layout_below="@+id/lblComisionName"
        android:layout_marginTop="18dp"
        android:text="Fecha de Inicio"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/lblComisionStartDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignRight="@+id/textView11"
        android:layout_below="@+id/textView1"
        android:layout_marginTop="16dp"
        android:text="01/01/1999"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lblComisionStartDate"
        android:layout_alignRight="@+id/lblComisionStartDate"
        android:layout_below="@+id/lblComisionStartDate"
        android:layout_marginTop="20dp"
        android:text="Fecha de Finalizacion"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/lblComisionEndDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView33"
        android:layout_alignRight="@+id/textView33"
        android:layout_below="@+id/textView33"
        android:layout_marginTop="15dp"
        android:text="01/01/1999"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView66"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lblComisionEndDate"
        android:layout_alignRight="@+id/lblComisionEndDate"
        android:layout_below="@+id/lblComisionEndDate"
        android:layout_marginTop="21dp"
        android:text="Personal involucrado"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="20dp" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray" />

    <ListView
        android:id="@+id/ListViewComisionDetail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView66"
        android:layout_alignRight="@+id/textView66"
        android:layout_below="@+id/textView66"
        android:layout_marginTop="16dp" >
    </ListView>

</LinearLayout>